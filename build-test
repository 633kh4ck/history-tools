eosio-cpp -c -o lib-placeholders.wasm ../src/lib-placeholders.cpp
eosio-cpp -c -o test-server.wasm ../src/test-server.cpp
eosio-cpp -c -o ex-token-server.wasm ../src/ex-token-server.cpp
eosio-cpp -c -o ex-chain-server.wasm ../src/ex-chain-server.cpp
eosio-cpp -c -o test-client.wasm ../src/test-client.cpp

/usr/local/eosio.cdt/bin/wasm-ld -e startup --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm test-server.wasm ex-token-server.wasm ex-chain-server.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O3 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o test-server.wasm --allow-undefined-file=../src/wasm-ql.imports -lc++ -lc -leosio

/usr/local/eosio.cdt/bin/wasm-ld -e create_request --export decode_response --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm test-client.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O0 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o test-client.wasm --allow-undefined-file=../src/client.imports -lc++ -lc -leosio
