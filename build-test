# eosio-cpp -Os -c -o ex-token-client.o  ../wasms/token/token-client.cpp                          -I ../src/wasm -I ../libraries/eosiolib/wasmql -I ../external/abieos/src -I ../external/abieos/external/date/include -I ../external/abieos/external/rapidjson/include -DRAPIDJSON_64BIT=1 -DRAPIDJSON_48BITPOINTER_OPTIMIZATION=1

eosio-ld -o legacy-server.wasm ex-legacy-server.o lib-placeholders.o -fquery-server --eosio-imports ../src/wasm/wasm-ql.imports
eosio-ld -o token-client.wasm ex-token-client.o lib-placeholders.o -fquery-client --eosio-imports ../src/wasm/wasm-ql.imports
eosio-ld -o chain-client.wasm ex-chain-client.o lib-placeholders.o -fquery-client --eosio-imports ../src/wasm/wasm-ql.imports
