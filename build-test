eosio-cpp -c -o lib-placeholders.o ../src/wasm/lib-placeholders.cpp -I ../external/abieos/src -I ../external/abieos/external/date/include -I ../external/abieos/external/rapidjson/include -DRAPIDJSON_64BIT=1 -DRAPIDJSON_48BITPOINTER_OPTIMIZATION=1
eosio-cpp -c -o ex-chain-server.o ../src/wasm/ex-chain-server.cpp -I ../external/abieos/src -I ../external/abieos/external/date/include -I ../external/abieos/external/rapidjson/include -DRAPIDJSON_64BIT=1 -DRAPIDJSON_48BITPOINTER_OPTIMIZATION=1
