eosio-cpp -c -o lib-placeholders.wasm ../src/wasm/lib-placeholders.cpp
eosio-cpp -c -o ex-chain-server.wasm ../src/wasm/ex-chain-server.cpp
eosio-cpp -c -o ex-token-server.wasm ../src/wasm/ex-token-server.cpp
eosio-cpp -c -o ex-chain-client.wasm ../src/wasm/ex-chain-client.cpp
eosio-cpp -c -o ex-token-client.wasm ../src/wasm/ex-token-client.cpp

/usr/local/eosio.cdt/bin/wasm-ld -e startup --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm ex-chain-server.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O3 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o chain-server.wasm --allow-undefined-file=../src/wasm/wasm-ql.imports -lc++ -lc -leosio

/usr/local/eosio.cdt/bin/wasm-ld -e startup --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm ex-token-server.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O3 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o token-server.wasm --allow-undefined-file=../src/wasm/wasm-ql.imports -lc++ -lc -leosio

/usr/local/eosio.cdt/bin/wasm-ld -e create_request --export decode_response --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm ex-chain-client.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O0 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o chain-client.wasm --allow-undefined-file=../src/wasm/client.imports -lc++ -lc -leosio

/usr/local/eosio.cdt/bin/wasm-ld -e create_request --export decode_response --export-table --gc-sections --strip-all -zstack-size=8192 --merge-data-segments lib-placeholders.wasm ex-token-client.wasm -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -stack-first --lto-O0 -L/usr/local/eosio.cdt/bin/../lib -L/usr/local/eosio.cdt/bin/../lib64 -o token-client.wasm --allow-undefined-file=../src/wasm/client.imports -lc++ -lc -leosio
